{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "pardhuadf123"
		},
		"LS_AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureBlobStorage'"
		},
		"LSaabsjsontocsv_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LSaabsjsontocsv'"
		},
		"LS_RESTAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://reqres.in/api/user/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "pardhuadf123_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureBlobStorage_connectionString": {
						"value": "[parameters('LS_AzureBlobStorage_connectionString')]"
					},
					"LSaabsjsontocsv_connectionString": {
						"value": "[parameters('LSaabsjsontocsv_connectionString')]"
					},
					"LS_RESTAPI_properties_typeProperties_url": {
						"value": "[parameters('LS_RESTAPI_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}